{
  "manifest_version": 2,

  "name": "Contextual",
  "description": "Easily find the relevant context in Google search results",
  "version": "0.1",

  "background": {
    "scripts": ["jquery-2.1.3.min.js", "EventPage.js"]
  },

  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery-2.1.3.min.js"]
    }
  ],

  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
